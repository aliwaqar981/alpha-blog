<h1>Listing all articles</h1>
<p>
  <%= link_to "Create new Article", new_article_path %>
</p> 
<table>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>

    <% @articles.each do |articles| %>
        <tr>
          <td><%= articles.title %></td>
          <td><%= articles.description %></td>
          <td><%= link_to "Edit", edit_article_path(articles)%></td>
          <td><%= link_to "Show", article_path(articles)%></td>
        </tr>
    <% end %>
</table>